<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="Lexical Checks"  priority="1600">
        <description>%LexicalChecks.desc</description>

        <rule-metadata name="%MethodLimit.name" internal-name="MethodLimit" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.MethodLimitCheck" />
            <description>%MethodLimit.desc</description>
            <property-metadata name="max" datatype="Integer" default-value="30">
                <description>The maximum number of allowed methods.</description>
            </property-metadata>
            <quickfix classname="net.sf.eclipsecs.sample.checks.MethodLimitQuickfix" />
        </rule-metadata>
        <rule-metadata internal-name="MeaninglessTerms" name="%MeaninglessTerms.name"
        	parent="TreeWalker">
        	<alternative-name internal-name="" /><description>%MeaninglessTerms.desc</description>
        </rule-metadata>
        <rule-metadata internal-name="MisspelledWords" name="%MisspelledWords.name"
        	parent="TreeWalker">
        	<alternative-name internal-name="" /><description>%MisspelledWords.desc</description>
        </rule-metadata>
        <rule-metadata internal-name="ExtremelyShortTerms" name="%ExtremelyShortTerms.name"
        	parent="TreeWalker">
        	<alternative-name internal-name="" /><description>%ExtremelyShortTerms.desc</description>
        	<property-metadata name="threshold" datatype="Integer"
        		default-value="3">
        	</property-metadata>
        </rule-metadata>
        <rule-metadata internal-name="OddGrammaticalStructure" name="%OddGrammaticalStructure.name"
        	parent="TreeWalker">
        	<alternative-name internal-name="" /><description>%OddGrammaticalStructure.desc</description>
        </rule-metadata>
        <rule-metadata internal-name="OverloadedIdentifiers" name="%OverloadedIdentifiers.name"
        	parent="TreeWalker">
        	<alternative-name internal-name="" /><description>%OverloadedIdentifiers.desc</description>
        </rule-metadata>
        <rule-metadata internal-name="UselessTypeIndication" name="%UselessTypeIndication.name"
        	parent="TreeWalker">
        	<alternative-name internal-name="" /><description>%UselessTypeIndication.desc</description>
        </rule-metadata>
    </rule-group-metadata>
</checkstyle-metadata>
